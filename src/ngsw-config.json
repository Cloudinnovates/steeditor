{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/*.bundle.css",
          "/*.bundle.js",
          "/*.chunk.js",
          "/favicon.ico",
          "/index.html"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    },
    {
      "name": "fonts",
      "resources": {
        "urls": ["https://fonts.googleapis.com/**"]
      }
    }
  ]
}
